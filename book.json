{
    "title": "Pembrock gitbook",
    "description": "Pembrock gitbook with docs",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    
        "plugins": [
          "splitter",
          "hide-element",
          "code",
          "theme-door",
          "-lunr",
          "-search",
          "-sharing",
          "search-pro-fixed"
        ],
        "variables": {
          "doorTheme": {

          }
        },
        "pluginsConfig": {
          "hide-element": {
            "elements": [".gitbook-link"]
          },
          "doorTheme": {
            "search-placeholder": "search",
            "logo": "./_media/logo.svg",
            "favicon": "./_media/favicon.ico"
          }
        }
}